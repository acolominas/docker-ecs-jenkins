{
    "family": "aci-ecstsk-jenkins-dev-euw1-01",
    "taskRoleArn": "arn:aws:iam::224322756201:role/aci-iamrole-jenkins-dev-euw1-01",
    "executionRoleArn": "arn:aws:iam::224322756201:role/aci-iamrole-jenkins-dev-euw1-01",
    "requiresCompatibilities" : ["FARGATE"],
    "networkMode"             : "awsvpc",
    "cpu"                      : "2048",
    "memory"                   : "4096",
    "containerDefinitions": [
        {
            "name": "jenkins",
            "image": "nginx",
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "mountPoints": [
              {
                "containerPath": "/var/jenkins_home",
                "sourceVolume": "jenkins-efs"
              }
            ],
            "volumesFrom": [],
            "environment": [
              { "name" : "JAVA_OPTS", "value" : "-Djenkins.install.runSetupWizard=false" }
            ],
            "secrets": [
              {
                "name": "ADMIN_PWD",
                "valueFrom": "arn:aws:secretsmanager:eu-west-1:224322756201:secret:aci-smsecrt-jenkins-dev-euw1-admin-FhLzOy"
              }
            ]
        }
    ],
    "volumes": [
      {
        "name" : "jenkins-efs",
        "efsVolumeConfiguration": {
          "fileSystemId": "fs-0c3d96d116126915e",
          "transitEncryption": "ENABLED",
          "authorizationConfig": {
            "accessPointId": "fsap-01daeb6310863e269",
            "iam": "ENABLED"
          }
        }
     }
   ]
}
